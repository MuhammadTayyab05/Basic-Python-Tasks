# Q48
dic={"a": 10, "b": 20, "c": 10, "d": 30}
print(dic.values())
set1=set(dic.values())
print(set1)

# Q49
str = "PYTHON"
reverse = ""
for i in str:
    reverse=i + reverse
print(reverse)

# 50
secert_num=5
while True:
    num1= float(input("enter the string :"))
    if num1 < secert_num
        print("Too low,try again")
    elif (num1 > secert_num):
        print("too high,try again")
    else:
        print("congratulations you guess the number")
    break
