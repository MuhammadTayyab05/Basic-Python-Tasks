print(tup1)

# Q44
dic={'harry' :81,
     'tayyab':61,
     'hassan':87
    }
for i in dic:
    val=dic.values()
print(i,min(val))


# Q45
var1= input(str("enter the string :"))
V=0
for i in var1:
    
    if i in ('AEIOUaeiou'):
        V +=1

print(f'there is vovels in the string',{V} )

# Q46
